@using Mettal.Models.ViewModels
@model Mettal.Models.ViewModels.TableView

@{
    ViewBag.Title = "Таблицы";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="admin">
    <div class="container">

        <h2>@ViewBag.Title <span class="btn-add-item">@Html.ActionLink("Добавить таблицу", "Update", null, new { @class = "btn btn-default" })</span> </h2>

        @Html.Partial("_TableList", Model.Tables)

    </div>
</div>

